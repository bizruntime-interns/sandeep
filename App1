using System;
using System.Collections;

namespace Application1
{
    class Program
    {
        static void Main(string[] args)
        {
            {
                ArrayList a1 = new ArrayList();
                a1.Add(1);
                a1.Add("Example");
                a1.Add(true);

                Console.WriteLine(a1[0]);
                Console.WriteLine(a1[1]);
                Console.WriteLine(a1[2]);
                Console.ReadKey();
            }
        }
    }
}
